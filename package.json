{
  "name": "atom-pwa",
  "version": "4.2.1",
  "author": "Horus Lugo <horusgoul@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently 'netlify dev' 'yarn netlify-cms-proxy-server'",
    "prebuild": "rimraf dist",
    "build": "yarn typecheck; vite build && workbox generateSW workbox-config.js",
    "lint": "eslint src admin --ext .ts,.tsx",
    "locales-coverage": "node scripts/build-locales-coverage.js",
    "locales-coverage:description": "node scripts/locales-coverage-description.js",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky install",
    "test": "jest --watchAll",
    "test:ci": "jest --maxWorkers=2",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "coverage": "yarn coverage:clean-all && (yarn test:ci; mv coverage jest-coverage) && (yarn cy:run; mv coverage cypress-coverage) && yarn coverage",
    "coverage:ci": "yarn coverage:clean && yarn coverage:make-dir && yarn coverage:copy-reports && yarn coverage:generate-report",
    "coverage:clean": "rm -rf coverage",
    "coverage:clean-all": "yarn coverage:clean && rm -rf jest-coverage cypress-coverage",
    "coverage:make-dir": "mkdir -p coverage",
    "coverage:copy-reports": "yarn coverage:copy-jest && yarn coverage:copy-cypress",
    "coverage:copy-jest": "cp jest-coverage/coverage-final.json coverage/jest-final.json",
    "coverage:copy-cypress": "cp cypress-coverage/coverage-final.json coverage/cypress-final.json",
    "coverage:generate-report": "nyc report --reporter lcov --reporter text --temp-dir coverage"
  },
  "lint-staged": {
    "**/*.{html,ts,tsx,json,scss,js,jsx}": [
      "prettier --write"
    ],
    "{admin,src}/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@sentry/react": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "animejs": "^3.2.1",
    "autobind-decorator": "^2.1.0",
    "classnames": "^2.2.5",
    "cypress": "^6.5.0",
    "hammerjs": "^2.0.8",
    "invariant": "^2.2.4",
    "netlify-cms-app": "^2.14.23",
    "netlify-identity-widget": "^1.9.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hyper-scroller": "^3.0.0-beta.5",
    "react-portal": "^4.2.1",
    "react-router-dom": "^5.2.0",
    "spycat": "^0.0.6",
    "workbox-precaching": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@cypress/code-coverage": "^3.9.2",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.3",
    "@types/animejs": "^3.1.2",
    "@types/classnames": "^2.2.11",
    "@types/hammerjs": "^2.0.39",
    "@types/invariant": "^2.2.34",
    "@types/jest": "^26.0.20",
    "@types/jest-in-case": "^1.0.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-portal": "^4.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vitejs/plugin-legacy": "^1.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "babel-jest": "^26.6.3",
    "babel-preset-react-app": "^10.0.0",
    "babel-preset-vite": "^1.0.3",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-unused-imports": "^1.1.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "jest-in-case": "^1.0.2",
    "jest-useragent-mock": "^0.1.1",
    "last-commit-log": "^3.1.1",
    "lint-staged": "^10.5.4",
    "netlify-cli": "^3.8.5",
    "netlify-cms-proxy-server": "^1.3.11",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "react-app-polyfill": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "typescript": "^4.1.5",
    "vite": "^2.0.1",
    "vite-plugin-istanbul": "^2.0.0",
    "vite-plugin-svgr": "^0.2.0",
    "whatwg-fetch": "^3.6.1",
    "workbox-cli": "^6.1.0"
  },
  "resolutions": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
