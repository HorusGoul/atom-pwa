{
  "name": "atom-pwa",
  "version": "4.4.0",
  "author": "Horus Lugo <horusgoul@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently 'netlify dev' 'pnpm netlify-cms-proxy-server'",
    "prebuild": "rimraf dist",
    "build": "pnpm typecheck; vite build && workbox generateSW workbox-config.js",
    "lint": "eslint src admin --ext .ts,.tsx",
    "locales-coverage": "node scripts/build-locales-coverage.js",
    "locales-coverage:description": "node scripts/locales-coverage-description.js",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky install",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "cy:install": "cypress install",
    "cy:run": "cypress run",
    "cy:open": "cypress open"
  },
  "lint-staged": {
    "**/*.{html,ts,tsx,json,scss,js,jsx}": [
      "prettier --write"
    ],
    "{admin,src}/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@sentry/react": "^7.33.0",
    "@sentry/tracing": "^7.33.0",
    "animejs": "^3.2.1",
    "body-scroll-lock": "^3.1.5",
    "classnames": "^2.3.1",
    "cypress": "^6.5.0",
    "focus-trap-react": "^8.4.2",
    "hammerjs": "^2.0.8",
    "history": "^4",
    "immer": "^9.0.6",
    "invariant": "^2.2.4",
    "minisearch": "^3.0.2",
    "netlify-cms-app": "2.14.32",
    "netlify-cms-core": "^2.55.2",
    "netlify-identity-widget": "^1.9.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-component-pack": "^1.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hyper-scroller": "^3.1.0-beta.2",
    "react-router-dom": "^5.2.0",
    "use-debounce": "^6.0.1",
    "use-immer": "^0.5.1",
    "workbox-precaching": "^6.5.4",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@cypress/code-coverage": "^3.9.4",
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@testing-library/user-event": "^13.1.1",
    "@types/animejs": "^3.1.3",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/classnames": "^2.2.11",
    "@types/hammerjs": "^2.0.39",
    "@types/history": "^4",
    "@types/invariant": "^2.2.34",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitest/coverage-istanbul": "^0.28.1",
    "concurrently": "^6.0.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "jest-useragent-mock": "^0.1.1",
    "jsdom": "^21.1.0",
    "last-commit-log": "^3.1.1",
    "lint-staged": "^10.5.4",
    "netlify-cli": "^3.15.1",
    "netlify-cms-proxy-server": "^1.3.12",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "react-app-polyfill": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "typescript": "^4.9.4",
    "vite": "4.0.4",
    "vite-plugin-istanbul": "^4.0.0",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.28.1",
    "whatwg-fetch": "^3.6.1",
    "workbox-cli": "^6.1.0"
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "packageManager": "pnpm@7.14.1",
  "pnpm": {
    "patchedDependencies": {
      "@types/testing-library__jest-dom@5.14.5": "patches/@types__testing-library__jest-dom@5.14.5.patch"
    }
  }
}
