{
  "name": "atom-pwa",
  "version": "4.2.0-alpha.1",
  "author": "Horus Lugo <horusgoul@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently 'netlify dev' 'yarn netlify-cms-proxy-server'",
    "prebuild": "rimraf dist",
    "build": "yarn typecheck; vite build && workbox generateSW workbox-config.js",
    "lint": "eslint src admin --ext .ts,.tsx",
    "locales-coverage": "node scripts/build-locales-coverage.js",
    "locales-coverage:description": "node scripts/locales-coverage-description.js",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "**/*.{html,ts,tsx,json,scss,js,jsx}": [
      "prettier --write"
    ],
    "{admin,src}/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "animejs": "^3.2.1",
    "autobind-decorator": "^2.1.0",
    "classnames": "^2.2.5",
    "firebase": "^8.2.7",
    "hammerjs": "^2.0.8",
    "invariant": "^2.2.4",
    "netlify-cms-app": "^2.14.21",
    "netlify-identity-widget": "^1.9.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hyper-scroller": "next",
    "react-portal": "^4.2.1",
    "react-router-dom": "^5.2.0",
    "spycat": "^0.0.5",
    "workbox-precaching": "^6.1.0"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.2",
    "@types/classnames": "^2.2.11",
    "@types/hammerjs": "^2.0.37",
    "@types/invariant": "^2.2.34",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-portal": "^4.0.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vitejs/plugin-legacy": "^1.3.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.0.9",
    "last-commit-log": "^3.1.1",
    "lint-staged": "^10.5.4",
    "netlify-cli": "^3.7.1",
    "netlify-cms-proxy-server": "^1.3.10",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.7",
    "typescript": "^4.1.5",
    "vite": "^2.0.0-beta.69",
    "vite-plugin-svgr": "^0.2.0",
    "workbox-cli": "^6.1.0"
  }
}
